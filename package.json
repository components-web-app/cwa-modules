{
  "name": "@cwa/nuxt-module",
  "version": "1.0.0",
  "type": "module",
  "description": "Nuxt Module for Silverback's Components Web Apps and API Components Bundle API",
  "license": "MIT",
  "author": "Daniel West <daniel@silverback.is>",
  "repository": "https://github.com/components-web-app/cwa-nuxt-module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/module/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "audit": "improved-yarn-audit --retry-on-network-failure --ignore-dev-deps --min-severity moderate",
    "build": "tsc && yarn copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.vue src/**/*.sass src/core/assets/sass/fonts/**/* src/core/assets/images/* dist/",
    "clean": "rm -rf dist && rm -rf tests/fixture/.nuxt && rm -rf demo/.nuxt",
    "dev": "nuxt demo",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint-fix": "yarn lint --fix",
    "release": "yarn clean && yarn build && standard-version && git push --follow-tags && npm publish",
    "demo:build": "nuxt build demo",
    "demo:analyze": "nuxt build demo --analyze",
    "demo:start": "nuxt start demo",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/**/*.spec.ts --detectOpenHandles",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "api": "node scripts/mock-api",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate",
    "css-build": "node-sass --omit-source-map-url sass/style.sass css/cwa.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "dependencies": {
    "@nuxt/components": "^2.2.0",
    "@nuxtjs/auth-next": "^5.0.0-1641419856.31900b3",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/svg": "^0.4.0",
    "@popperjs/core": "^2.11.2",
    "bluebird": "^3.7.2",
    "bulma": "^0.9.4",
    "consola": "^2.13.0",
    "css-loader": "^5.2.0",
    "har-validator": "^5.1.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isobject": "^3.0.2",
    "lodash.set": "^4.3.2",
    "milligram": "^1.4.0",
    "moment": "^2.27.0",
    "slugify": "^1.4.6",
    "style-loader": "^1.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "vue-flatpickr-component": "^8",
    "vuedraggable": "^2.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.16.7",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/vue-app-edge": "^2.16.0-26916516.94fff623",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/node-sass": "^4.11.3",
    "all-contributors-cli": "^6.16.0",
    "axiosist": "^0.10.0",
    "babel-jest": "^27.4.5",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.3.0",
    "cypress": "^10.7.0",
    "eslint": "8.22",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fibers": "^5.0.0",
    "improved-yarn-audit": "^2.3.3",
    "jest": "^27",
    "jsdom": "^16.2.2",
    "mock-json-schema": "^1.0.8",
    "nuxt": "^2.15.8",
    "prettier": "^2.2.1",
    "quill": "^1.3.7",
    "sass": "1.44",
    "sass-loader": "^10",
    "sass-resources-loader": "^2.2.4",
    "ssl-root-cas": "^1.3.1",
    "standard-version": "^9.3.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "vue-loader": "^15.9.6"
  }
}

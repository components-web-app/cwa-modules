{
  "name": "@cwa/nuxt-module",
  "version": "1.0.0",
  "type": "module",
  "description": "Nuxt Module for Silverback's Components Web Apps and API Components Bundle API",
  "license": "MIT",
  "author": "Daniel West <daniel@silverback.is>",
  "repository": "https://github.com/components-web-app/cwa-nuxt-module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/module/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "audit": "improved-yarn-audit --retry-on-network-failure --ignore-dev-deps --min-severity moderate",
    "build": "tsc && yarn copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.vue src/**/*.sass src/core/assets/fonts/**/* src/core/assets/images/* dist/",
    "clean": "rm -rf dist && rm -rf tests/fixture/.nuxt && rm -rf demo/.nuxt",
    "dev": "nuxt-ts demo",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint-fix": "yarn lint --fix",
    "release": "yarn clean && yarn build && standard-version && git push --follow-tags && npm publish",
    "demo:build": "nuxt-ts build demo",
    "demo:start": "nuxt-ts start demo",
    "test": "jest src/**/*.spec.ts --detectOpenHandles",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "api": "node scripts/mock-api",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate"
  },
  "dependencies": {
    "@nuxt/components": "^2.2.0",
    "@nuxtjs/auth-next": "5.0.0-1640380807.e0bda01",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/svg": "^0.1.12",
    "bluebird": "^3.7.2",
    "consola": "^2.13.0",
    "css-loader": "^5.2.0",
    "har-validator": "^5.1.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "milligram": "^1.4.0",
    "moment": "^2.27.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.37",
    "sass-loader": "10",
    "sass-resources-loader": "^2.0.3",
    "slugify": "^1.4.6",
    "style-loader": "^1.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "vue-flatpickr-component": "^8",
    "vuedraggable": "^2.24.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@nuxt/types": "^0.7.9",
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxt/vue-app-edge": "^2.16.0-26916516.94fff623",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^17.0.5",
    "@types/node-sass": "^4.11.1",
    "all-contributors-cli": "^6.16.0",
    "axiosist": "^0.10.0",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.3.0",
    "cypress": "9.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "improved-yarn-audit": "^2.3.3",
    "jest": "^26.1.0",
    "jsdom": "^16.2.2",
    "mock-json-schema": "^1.0.8",
    "nuxt": "^2.15.7",
    "prettier": "^2.2.1",
    "quill": "^1.3.7",
    "ssl-root-cas": "^1.3.1",
    "standard-version": "^9.3.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.8.3",
    "uuid": "^8.3.2",
    "vue-loader": "^15.9.6"
  }
}
